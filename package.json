{
    "name": "simple-time-ago",
    "version": "0.1.0",
    "author": "velut",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/simple-time-ago.esm.js",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "test:cov": "tsdx test --coverage",
        "test:watch": "tsdx test --watchAll --coverage",
        "lint": "tsdx lint src test",
        "prepare": "tsdx build",
        "pre-push": "yarn lint && yarn build && yarn test:cov"
    },
    "peerDependencies": {},
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint",
            "pre-push": "yarn pre-push"
        }
    },
