        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5",
        "arrowParens": "always"
    },
    "devDependencies": {
        "husky": "^4.2.5",
        "tsdx": "^0.13.2",
        "tslib": "^2.0.0",
        "typescript": "^3.9.5"
    },
    "keywords": [
        "time",
        "ago",
        "date",
        "format",
        "relative",
        "human",
        "readable",
        "typescript"
    ]
